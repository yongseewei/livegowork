<h1>Edit profile</h1>
<div id="edit" class="edit">


<%= form_for @user do |form| %>
<%= render partial: '/users/form', object: form %>

<div class="profile_image">
<%= form.label :image %>
<%= form.file_field :avatar %>
</div>

<div class="submit-field">
  <%= form.submit 'update' %>
</div>

<% end %>
</div>
